{
  "expo": {
    "name": "Visite Itu",
    "slug": "visite-itu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logoAppItu.jpg",
    "scheme": "visiteitu",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logoAppItu.jpg"      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logoAppItu.jpg"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
